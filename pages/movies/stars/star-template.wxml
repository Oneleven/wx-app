<template name="starTemplate">
  <view class="star-container">
    <view class="stars">
      <block wx:for="{{stars}}" wx:key="{{index}}">
        <image wx:if="{{item}}" src="/images/icons/star.png"></image>
        <image wx:else src="/images/icons/none-star.png"></image>
      </block>
    </view>
    <text class="star-score">{{averageScore}}</text>
  </view>
</template>